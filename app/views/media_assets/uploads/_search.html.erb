<%= form_search(path: upload_media_assets_path, policy: policy(UploadMediaAsset)) do |f| %>
  <%= f.user(:creator) %>
  <%= f.input(:checksum) %>
  <%= f.input(:md5, label: "MD5") %>
  <%= f.input(:file_ext, label: "Type") %>
  <%= f.input(:pixel_hash, label: "Pixel Hash") %>
  <%= f.input(:status, collection: UploadMediaAsset.statuses) %>
  <%= f.input(:status_message_matches, label: "Status Message") %>
  <%= f.input(:upload_id, label: "Upload") %>
  <%= f.input(:post_id, label: "Post") %>
<% end %>
