<h1>Popular: <%= date_range_description(@date, @scale, @min_date, @max_date) %></h1>

<%= @post_set.presenter.nav_links(self) %>

<%= render("posts/partials/common/inline_blacklist") %>

<div id="posts" class="user-disable-cropped-<%= FemboyFans.config.enable_image_cropping? && CurrentUser.user.disable_cropped_thumbnails? %>">
  <div id="posts-container">
    <%= @post_set.presenter.post_previews_html(self, show_cropped: true) %>
  </div>
</div>

<%= render(partial: "popular/secondary_links") %>
<% content_for(:secondary_links) { "|" } %>
<%= render("posts/partials/common/secondary_links") %>

<% content_for(:page_title) do %>
  Popular Uploads for <%= date_range_description(@date, @scale, @min_date, @max_date) %>
<% end %>

<% content_for(:html_header) do %>
  <meta name="description" content="The most popular posts per day">
<% end %>
